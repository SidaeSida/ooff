{
  "regions": ["sin1"],
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "ourownfilmfestival.com" }],
      "destination": "https://www.ourownfilmfestival.com/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains; preload"
        }
      ]
    }
  ]
}
